<launch>
	<rosparam command="load"
		file="$(find usonic_sensor)/launch/parameter.yaml" />
        
        <node pkg="tf"
                type="static_transform_publisher"
                name="sensor"
                args=" 0 0 0  0 0 0 /vicon/hans/hans usonic_sensor 100"
        />


	<node pkg="usonic_sensor"
		name="maxbotix_sensor_node"
		type="maxbotix_sensor_node"
		output="screen"
	/>
</launch>
